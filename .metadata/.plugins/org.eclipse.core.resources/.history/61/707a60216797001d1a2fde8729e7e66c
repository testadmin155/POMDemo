package PageObject;

import org.openqa.selenium.By;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;

public class HomePage {

	//driver.findElement(By.id("code")).sendKeys("9999");
	@FindBy(id="code")
	WebElement Code;
	
	public void EnterCode() {
		Code.sendKeys("9999");
	}
}
